<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen_228dp"
    android:layout_marginTop="@dimen/dimen_15dp"
    android:layout_marginBottom="@dimen/dimen_15dp"
    android:orientation="vertical" >

     <LinearLayout
         android:id="@+id/top123"
         android:layout_width="@dimen/dimen_300dp"
         android:layout_height="@dimen/dimen_228dp"
         android:layout_alignParentBottom="true"
         android:layout_alignParentTop="true"
         android:layout_centerHorizontal="true"
         android:layout_weight="2"
         android:orientation="vertical"
         android:background="@drawable/v3_border_shadow_xml"
         android:weightSum="2" >
         <!-- pm_post_box -->
         <LinearLayout
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:layout_marginTop="@dimen/dimen_10dp"
	         android:layout_marginLeft="@dimen/dimen_10dp"
	         android:layout_marginRight="@dimen/dimen_10dp"
	         android:orientation="horizontal">

	         	<org.acv.bynal.views.BaseBoldTextView
	         	    android:id="@+id/project_postlist_title"
	         	    android:layout_width="0dp"
	         	    android:layout_height="wrap_content"
	         	    android:gravity="left"
	         	    android:layout_weight="0.8"
	         	    android:textColor="#595959"
	         	    android:textSize="@dimen/dimen_10dp"
	         	    android:text="title" />

	         	<org.acv.bynal.views.BaseTextView
	         	    android:id="@+id/project_postlist_status"
	         	    android:layout_width="0dp"
	         	    android:layout_height="wrap_content"
	         	    android:gravity="right"
	         	    android:layout_weight="0.2"
	         	    android:textSize="@dimen/dimen_10dp"
	         	    android:text="stutus" />

         </LinearLayout>
         <LinearLayout
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:orientation="vertical">

	         	<org.acv.bynal.views.BaseTextView
	         	    android:id="@+id/project_postlist_time_modified"
	         	    android:layout_width="wrap_content"
	         	    android:layout_height="wrap_content"
	         	    android:layout_marginTop="@dimen/dimen_5dp"
	         		android:layout_marginLeft="@dimen/dimen_10dp"
	         		android:layout_marginRight="@dimen/dimen_10dp"
	         	    android:gravity="left"
	         	    android:textColor="#595959"
	         	    android:textSize="@dimen/dimen_10dp"
	         	    android:text="最終更新日時 " />

	         	<org.acv.bynal.views.BaseTextView
	         	    android:id="@+id/project_postlist_time_created"
	         	    android:layout_width="fill_parent"
	         	    android:layout_height="wrap_content"
	         	    android:layout_marginTop="@dimen/dimen_5dp"
	         		android:layout_marginLeft="@dimen/dimen_10dp"
	         		android:layout_marginRight="@dimen/dimen_10dp"
	         	    android:gravity="left"
	         	    android:textColor="#595959"
	         	    android:textSize="@dimen/dimen_10dp"
	         	    android:text="申請日時 " />
         </LinearLayout>
          <LinearLayout
	         android:layout_width="fill_parent"
	         android:layout_height="wrap_content"
	         android:layout_marginTop="@dimen/dimen_10dp"
	         android:layout_marginLeft="@dimen/dimen_10dp"
	         android:layout_marginRight="@dimen/dimen_10dp"
	         android:orientation="horizontal">

              <ImageView
                 android:id="@+id/homeitem_img"
                 android:layout_width="@dimen/dimen_80dp"
                 android:layout_height="@dimen/dimen_60dp"
                 android:background="@drawable/noimg"
                 android:scaleType="centerCrop"/>

	         	<org.acv.bynal.views.BaseTextView
	         	    android:id="@+id/project_postlist_desc"
	         	    android:layout_width="@dimen/dimen_198dp"
	         	    android:layout_height="wrap_content"
	         	    android:layout_marginLeft="@dimen/dimen_5dp"
	         	    android:gravity="left"
	         	    android:textSize="@dimen/dimen_10dp"
	         	    android:textColor="#595959"
	         	    android:text="desc desc desc desc desc" />

         </LinearLayout>
        <!-- icon001 -->
        <LinearLayout
        android:id="@+id/homeitem_content_1"
        android:layout_width="@dimen/dimen_300dp"
        android:layout_height="@dimen/dimen_16dp"
        android:layout_marginTop="@dimen/dimen_5dp"
        android:orientation="horizontal"
        android:weightSum="3" >

        <LinearLayout
            android:id="@+id/homeitem_content_step1"
            android:layout_width="@dimen/dimen_150dp"
            android:layout_height="@dimen/dimen_16dp"
            android:layout_marginLeft="@dimen/dimen_10dp"
            android:gravity="center|left"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/homeitem_img_1"
                android:layout_width="@dimen/dimen_16dp"
                android:layout_height="@dimen/dimen_16dp"
                android:background="@drawable/home_p1" />

            <org.acv.bynal.views.BaseBoldTextView
                android:id="@+id/homeitem_txtname_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="BaseTextView"
                android:textColor="#000000"
                android:textSize="@dimen/dimen_10dp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/homeitem_content_step2"
            android:layout_width="@dimen/dimen_70dp"
            android:layout_height="@dimen/dimen_16dp"
            android:gravity="center|left"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/homeitem_img_2"
                android:layout_width="@dimen/dimen_16dp"
                android:layout_height="@dimen/dimen_16dp"
                android:background="@drawable/home_p2" />

            <org.acv.bynal.views.BaseBoldTextView
                android:id="@+id/homeitem_txtname_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="BaseTextView"
                android:textColor="#000000"
                android:textSize="@dimen/dimen_10dp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/homeitem_content_step3"
            android:layout_width="@dimen/dimen_70dp"
            android:layout_height="@dimen/dimen_16dp"
            android:gravity="center|left"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/homeitem_img_3"
                android:layout_width="@dimen/dimen_16dp"
                android:layout_height="@dimen/dimen_16dp"
                android:background="@drawable/home_p3" />

            <org.acv.bynal.views.BaseBoldTextView
                android:id="@+id/homeitem_txtname_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingLeft="5dp"
                android:text="BaseTextView"
                android:textColor="#000000"
                android:textSize="@dimen/dimen_10dp"
                android:textStyle="bold" />
        </LinearLayout>
    </LinearLayout>
        <!-- icon001 -->
     </LinearLayout>
     <LinearLayout
         android:layout_width="wrap_content"
         android:layout_height="@dimen/dimen_22dp"
         android:layout_marginBottom="@dimen/dimen_10dp"
         android:layout_alignParentBottom="true"
         android:layout_centerHorizontal="true"
         android:orientation="horizontal" >

         <org.acv.bynal.views.BaseTextView
             android:id="@+id/pm_button_video"
             android:layout_width="@dimen/dimen_50dp"
             android:layout_height="@dimen/dimen_20dp"
             android:layout_marginLeft="@dimen/dimen_2dp"
             android:layout_marginRight="@dimen/dimen_2dp"
             android:background="@drawable/button_pm_post__xml"
             android:gravity="center"
             android:text="ビデオ"
             android:textColor="#ffffff"
             android:textSize="@dimen/dimen_8dp"
             android:textStyle="bold"
             android:visibility="visible" />

         <org.acv.bynal.views.BaseTextView
             android:id="@+id/pm_button_preview"
             android:layout_width="@dimen/dimen_70dp"
             android:layout_height="@dimen/dimen_20dp"
             android:layout_marginLeft="@dimen/dimen_2dp"
             android:layout_marginRight="@dimen/dimen_2dp"
             android:background="@drawable/button_pm_post__xml"
             android:gravity="center"
             android:text="プレビュー"
             android:textColor="#ffffff"
             android:textSize="@dimen/dimen_8dp"
             android:textStyle="bold"
             android:visibility="visible" />
         <org.acv.bynal.views.BaseTextView
            android:id="@+id/pm_button_support"
            android:layout_width="@dimen/dimen_70dp"
            android:layout_height="@dimen/dimen_20dp"
            android:background="@drawable/button_pm_post__xml"
            android:gravity="center"
            android:text="支援額明細"
            android:textColor="#ffffff"
            android:layout_marginLeft="@dimen/dimen_2dp"
             android:layout_marginRight="@dimen/dimen_2dp"
            android:visibility="gone"
            android:textSize="@dimen/dimen_8dp"
            android:textStyle="bold" />

        <org.acv.bynal.views.BaseTextView
            android:id="@+id/pm_button_delivery"
            android:layout_width="@dimen/dimen_80dp"
            android:layout_height="@dimen/dimen_20dp"
            android:background="@drawable/button_pm_post__xml"
            android:gravity="center"
            android:text="エンジェル一覧"
            android:textColor="#ffffff"
            android:layout_marginLeft="@dimen/dimen_2dp"
             android:layout_marginRight="@dimen/dimen_2dp"
            android:visibility="visible"
            android:textSize="@dimen/dimen_8dp"
            android:textStyle="bold" />
        <org.acv.bynal.views.BaseTextView
            android:id="@+id/pm_button_delete"
            android:layout_width="@dimen/dimen_50dp"
            android:layout_height="@dimen/dimen_20dp"
            android:layout_weight="0.25"
            android:background="@drawable/button_pm_post__xml"
            android:gravity="center"
            android:text="削除"
            android:textColor="#ffffff"
            android:layout_marginLeft="@dimen/dimen_2dp"
             android:layout_marginRight="@dimen/dimen_2dp"
            android:visibility="visible"
            android:textSize="@dimen/dimen_8dp"
            android:textStyle="bold" />
     </LinearLayout>

</RelativeLayout>