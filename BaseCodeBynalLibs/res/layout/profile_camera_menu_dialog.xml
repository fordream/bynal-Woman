<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/profile_camera_menu_dialog_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/profile_transparent"
    android:gravity="center"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="@dimen/dimen_182dp"
        android:layout_height="@dimen/dimen_108dp"
        android:layout_centerHorizontal="true"
        android:background="@drawable/profile_bg_popup"
        android:gravity="center"
        android:orientation="vertical" >

        <org.acv.bynal.views.BaseBoldTextView
            android:id="@+id/profile_camera_menu_dialog_camera001"
            android:layout_width="@dimen/dimen_170dp"
            android:layout_height="@dimen/dimen_40dp"
            android:background="@drawable/profile_popup_upload01"
            android:gravity="center|center_vertical"
            android:text="カメラから投稿する"
            android:textColor="#ffffff"
            android:textSize="@dimen/dimen_11dp" />

        <org.acv.bynal.views.BaseBoldTextView
            android:id="@+id/profile_camera_menu_dialog_camera002"
            android:layout_width="@dimen/dimen_170dp"
            android:layout_height="@dimen/dimen_40dp"
            android:layout_marginTop="@dimen/dimen_10dp"
            android:background="@drawable/profile_popup_upload02"
            android:gravity="center|center_vertical"
            android:text="カメラロールから投稿する"
            android:textColor="#ffffff"
            android:textSize="@dimen/dimen_11dp" />
    </LinearLayout>

</LinearLayout>