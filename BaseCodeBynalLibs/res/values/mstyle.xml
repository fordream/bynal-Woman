<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="bynal_parent_text"></style>

    <style name="bynal_text">
        <item name="android:background">@drawable/message_message_3</item>
        <item name="android:textSize">@dimen/dimen_16dp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:typeface">normal</item>
    </style>

    <style name="bynal_text_bold">
        <item name="android:background">@drawable/message_message_3</item>
